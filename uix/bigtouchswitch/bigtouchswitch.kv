<BigTouchSwitch>:
    size_hint: None, None
    size: switch.size[0], switch.size[1]
    MDSwitch:
        id: switch
        width:self.parent.switch_width
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        active: self.parent.active
        on_active: self.parent.on_active
        icon_active: self.parent.icon_active
        icon_inactive: self.parent.icon_inactive